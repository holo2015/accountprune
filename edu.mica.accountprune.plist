<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>edu.mica.accountprune</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/accountprune</string>
		<string>-P</string>
	</array>
    	<key>StartCalendarInterval</key>
    	<dict>
        	<key>Hour</key>
        	<integer>2</integer>
        	<key>Minute</key>
        	<integer>0</integer>
    	</dict>
	<key>StandardOutPath</key>
	<string>/var/log/accountprune.log</string>
</dict>
</plist>

